  "scripts": {
    "start": "set PORT=4500 && craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "electron": "electron ./electron/main.js",
    "electron-dev": "react-scripts start & electron ./electron/main.js",
    "lint": "eslint --ext .tsx,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "start:development": "env-cmd -f .env.dev npm-run-all start",
    "build:production": "env-cmd -f .env.prod npm run build",
    "build:stage": "env-cmd -f .env.stage npm run build",
    "prettier": "prettier . --ignore-path .gitignore",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "type:check": "tsc",
    "validate": "run-p lint type:check format:check"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "autoprefixer": "^10.4.14",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.js": "eslint --fix ",
    "*.{js,css,md,html,json}": "prettier --cache --write",
    "*.{tsx,ts}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
